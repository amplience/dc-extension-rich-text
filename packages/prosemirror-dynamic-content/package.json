{
  "name": "@dc-extension-rich-text/prosemirror-dynamic-content",
  "version": "0.1.0",
  "description": "Prosemirror node that allows embedding a DynamicContent image link in a document.",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run clean && npm run build:ts && npm run build:copy-css",
    "build:ts": "tsc -p tsconfig.json",
    "build:copy-css": "copyfiles -u 1 src/**/*.scss dist",
    "fix": "npm run fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "clean": "rm -rf dist",
    "storybook": "start-storybook -p 6007",
    "build-storybook": "build-storybook",
    "test": "jest",
    "watch": "npm run clean && npm run build && npm run test -- --watch"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "dependencies": {
    "@dc-extension-rich-text/common": "0.1.0",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@material/react-material-icon": "^0.15.0",
    "clsx": "^1.1.0",
    "dc-extensions-sdk": "^1.0.1",
    "prosemirror-example-setup": "^1.0.1",
    "prosemirror-model": "^1.7.4",
    "prosemirror-schema-basic": "^1.0.1",
    "prosemirror-state": "^1.2.4",
    "prosemirror-view": "^1.11.7",
    "unofficial-dynamic-content-ui": "^0.2.3"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@types/jest": "^24.0.18",
    "copyfiles": "^2.1.1",
    "jest": "^24.9.0",
    "ts-jest": "^24.0.2",
    "typescript": "^5.9.3",
    "@storybook/html": "^5.1.11"
  }
}
